<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abirshukla.RingStop" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.abirshukla.RingStop.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.abirshukla.RingStop.add"
            android:label="@string/title_activity_add" >
        </activity>
        <activity
            android:name="com.example.abirshukla.RingStop.add2"
            android:label="@string/title_activity_add2" >
        </activity>
        <activity
            android:name="com.example.abirshukla.RingStop.add3"
            android:label="@string/title_activity_add3" >
        </activity>
        <activity
            android:name="com.example.abirshukla.RingStop.checkStatus"
            android:label="@string/title_activity_check_status" >
        </activity>
    </application>

</manifest>
